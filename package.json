{
  "name": "amsmarmita-front",
  "version": "1.42.00",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run prebuild.prod && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prebuild.prod": "node git.version.js && node configenv.js",
    "build:prod": "npm run prebuild.prod && ng build --configuration production",
    "build:dev": " npm run prebuild.prod && ng build --configuration development",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "build:stats": "ng build --configuration production --stats-json",
    "analyzeb": "yarn build:stats && yarn analyze",
    "compodoc:build": "npx compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "npx compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "npx compodoc -s",
    "compodoc": "npx compodoc -p tsconfig.doc.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.1",
    "@angular/cdk": "^18.0.1",
    "@angular/common": "^18.0.1",
    "@angular/compiler": "^18.0.1",
    "@angular/core": "^18.0.1",
    "@angular/forms": "^18.0.1",
    "@angular/material": "^18.0.1",
    "@angular/material-date-fns-adapter": "^18.0.1",
    "@angular/platform-browser": "^18.0.1",
    "@angular/platform-browser-dynamic": "^18.0.1",
    "@angular/router": "^18.0.1",
    "dotenv": "^16.4.5",
    "file-saver": "^2.0.5",
    "imagekitio-angular": "^4.0.0",
    "js-sha256": "^0.11.0",
    "jwt-decode": "^4.0.0",
